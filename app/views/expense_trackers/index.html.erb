<p style="color: green"><%= notice %></p>

<h1>Travel Expense Tracker</h1>
<div>
  <%= link_to "New expense", new_expense_tracker_path %>
</div>

<table class="table">
  <thead>
  <tr>
    <th scope="col">Date</th>
    <th scope="col">Place</th>
    <th scope="col">Category</th>
    <th scope="col">Amount</th>
  </tr>
  </thead>
  <% @expense_trackers.each do |expense_tracker| %>
  <tbody>
  <tr>
      <td><%= expense_tracker.date %></td>
      <td><%= expense_tracker.place %></td>
      <td><%= expense_tracker.category %></td>
      <td><%= expense_tracker.amount %></td>
      <td> <%= link_to "Show this expense", expense_tracker %></td>
  <% end %>
  </tr>
  </tbody>
</table>



